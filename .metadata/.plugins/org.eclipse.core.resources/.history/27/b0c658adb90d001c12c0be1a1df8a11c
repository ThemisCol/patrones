<?xml version="1.0" encoding="UTF-8"?>
<project name="1.3 GUI" default="Crear GUI.jar">
    <description> Genera el jar de la GUI </description>
	<!-- variables -->
	<property name="base_man" location="orquestador\gui"/>
	<property name="base_gui" location="gui"/>
	<property name="destino_gui" location="orquestador\gui\gui.jar"/>
	<!--   guiar   -->
    <target name="Crear GUI.jar" description="crea el GUI.jar">
    	<jar destfile="${destino_gui}"  basedir="${base_gui}\bin" manifest="${base_man}\MANIFEST.MF"/>
    </target>
</project>

